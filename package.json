{
  "name": "leanda-test",
  "version": "3.0.0",
  "license": "MIT",
  "description": "Leanda end-to-end tests. Protractor, cucumber tests with typescript",
  "keywords": [
    "protractor",
    "cucumber",
    "typescript",
    "angular",
    "testing",
    "behaviour driven development",
    "bdd",
    "selenium",
    "webdriverJS",
    "gherkin",
    "automation testing"
  ],
  "scripts": {
    "ssp-smoke": "protractor tmp/config/config.js --baseUrl=https://ssp.your-company.com --cucumberOpts.tags=@ssp-smoke",
    "fvc-smoke": "protractor tmp/config/config.js --baseUrl=https://ssp.your-company.com --cucumberOpts.tags=@fvc-smoke",
    "all-features-prod": "protractor tmp/config/config.js --baseUrl=https://leanda.your-company.com --cucumberOpts.tags=@start-all-tests --cucumberOpts.tags=~@unstable",
    "all-features-uat": "protractor tmp/config/config.js --baseUrl=https://leanda.uat.your-company.com --cucumberOpts.tags=@start-all-tests --cucumberOpts.tags=~@unstable",
    "all-features-test": "protractor tmp/config/config.js --baseUrl=https://leanda.test.your-company.com --cucumberOpts.tags=@start-all-tests --cucumberOpts.tags=~@unstable",
    "all-features-dev": "protractor tmp/config/config.js --baseUrl=https://leanda.dev.your-company.com --cucumberOpts.tags=@start-all-tests --cucumberOpts.tags=~@unstable",
    "all-features": "protractor tmp/config/config.js --cucumberOpts.tags=@start-all-tests --cucumberOpts.tags=~@unstable",
    "machineLearning": "protractor tmp/config/config.js --baseUrl=https://leanda.dev.your-company.com --cucumberOpts.tags=@005 --cucumberOpts.tags=~@unstable",
    "smoke": "npm run tsc & npm run smoke-test",
    "smoke-test": "protractor tmp/config/config.js --cucumberOpts.tags=@smoke",
    "prod-smoke": "npm run tsc & protractor tmp/config/config.js --baseUrl=https://leanda.your-company.com --cucumberOpts.tags=@prod-smoke --cucumberOpts.tags=~@unstable",
    "pressp-smoke": "npm run tsc",
    "prefvc-smoke": "npm run tsc",
    "preall-features-prod": "npm run tsc",
    "preall-features-uat": "npm run tsc",
    "preall-features-test": "npm run tsc",
    "preall-features-dev": "npm run tsc",
    "preall-features": "npm run tsc",
    "test-all-features": "npm run all-features",
    "test-machineLearning": "npm run machineLearning",
    "test-ssp-smoke-prod": "npm run ssp-smoke",
    "test-fvc-smoke-prod": "npm run fvc-smoke",
    "tsc": "tsc"
  },
  "author": "Alexander Andriets <avandriets@gmail.com>",
  "dependencies": {
    "codelyzer": "^4.4.2"
  },
  "devDependencies": {
    "@types/cucumber": "^4.0.4",
    "@types/node": "^10.12.18",
    "@types/selenium-webdriver": "3.0.14",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "2.45.0",
    "cucumber": "5.1.0",
    "cucumber-pretty": "^1.5.0",
    "cucumber-tsflow": "3.2.0",
    "debug": "4.1.1",
    "glob": "7.1.3",
    "inflight": "1.0.6",
    "inherits": "2.0.3",
    "install": "^0.12.2",
    "minimatch": "3.0.4",
    "once": "1.4.0",
    "protractor": "^5.4.2",
    "protractor-cucumber-framework": "^6.1.1",
    "q": "1.5.1",
    "ts-node": "^7.0.1",
    "tslint": "5.12.1",
    "typescript": "^3.2.2",
    "wrappy": "1.0.2"
  }
}
