{
  "name": "nci-adult-match-ui",
  "version": "0.0.3",
  "license": "",
  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "track-configs": "git update-index --no-assume-unchanged src/environments/environment.ts && git update-index --no-assume-unchanged src/environments/environment.hmr.ts",
    "untrack-configs": "git update-index --assume-unchanged src/environments/environment.ts && git update-index --assume-unchanged src/environments/environment.hmr.ts",
    "config": "npm run untrack-configs && ts-node set-env.ts",
    "config-hmr": "npm run untrack-configs && ts-node set-env-hmr.ts",
    "start": "npm run config && ./node_modules/.bin/ng serve --host localhost --port 5555",
    "hmr": "npm run config-hmr && ./node_modules/.bin/ng serve --configuration hmr --port 5555",
    "build": "./node_modules/.bin/ng build",
    "test": "./node_modules/.bin/ng test",
    "lint": "./node_modules/.bin/ng lint",
    "e2e": "./node_modules/.bin/ng e2e",
    "build-deploy": "./node_modules/.bin/ng build --configuration=deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CBIIT/nci-adult-match-ui.git"
  },
  "bugs": {
    "url": "https://github.com/CBIIT/nci-adult-match-ui/issues"
  },
  "homepage": "https://github.com/CBIIT/nci-adult-match-ui#readme",
  "keywords": [
    "NCI",
    "MATCH"
  ],
  "author": "Rick Zakharov <rick.zakharov@gmail.com>",
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.0",
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/forms": "^7.2.0",
    "@angular/http": "^7.2.0",
    "@angular/platform-browser": "^7.2.0",
    "@angular/platform-browser-dynamic": "^7.2.0",
    "@angular/router": "^7.2.0",
    "@auth0/angular-jwt": "^2.1.0",
    "auth0-js": "^9.6.1",
    "auth0-lock": "^11.7.2",
    "bootstrap": "^3.3.7",
    "core-js": "^2.6.2",
    "d3": "5.7.0",
    "font-awesome": "^4.7.0",
    "jasmine": "^3.3.1",
    "moment": "^2.23.0",
    "ng2-charts": "1.6.0",
    "ngx-bootstrap": "^3.1.4",
    "ngx-nvd3": "^1.0.9",
    "ngx-toastr": "^9.1.1",
    "nvd3": "^1.8.6",
    "rxjs": "^6.3.3",
    "rxjs-tslint": "^0.1.6",
    "zone.js": "^0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.1",
    "@angular/cli": "^7.2.1",
    "@angular/compiler-cli": "^7.2.0",
    "@angular/language-service": "^7.2.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/auth0-lock": "^11.4.5",
    "@types/d3": "^5.5.0",
    "@types/jasmine": "^3.3.5",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "10.12.18",
    "@types/nvd3": "1.8.39",
    "codacy-coverage": "^3.4.0",
    "codelyzer": "^4.5.0",
    "eslint": "^5.12.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-spec-reporter": "0.0.32",
    "rxjs-tslint-rules": "^4.14.3",
    "ts-mockito": "^2.3.1",
    "ts-node": "^7.0.1",
    "tslint": "~5.12.1",
    "typescript": "3.2.2"
  }
}
